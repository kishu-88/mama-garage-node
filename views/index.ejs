    <script>
      const logoImage = document.querySelector(".mama-logo-img");

      const newImageSrc = "./img/garage-black.png"; // Replace with your new image URL

      // Get a reference to the .headTitle element
      const headTitle = document.querySelector(".headTitle");

      // Function to handle scroll event
      function handleScroll() {
        // Check if the user has scrolled beyond a certain point (e.g., 100 pixels from the top)
        if (window.scrollY > 50) {
          // Change the image source
          logoImage.src = newImageSrc;

          // Add the 'scroll-bg' class to apply the background color
          headTitle.classList.add("scroll-bg");
        } else {
          // Remove the 'scroll-bg' class if the user scrolls back to the top
          headTitle.classList.remove("scroll-bg");
          logoImage.src = "./img/garage-white.png";
        }
      }

      // Add a scroll event listener to the window
      window.addEventListener("scroll", handleScroll);
    </script>
